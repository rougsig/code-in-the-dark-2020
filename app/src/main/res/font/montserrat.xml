<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  tools:ignore="UnusedAttribute">
  <!-- How use and set fonts see wiki page "Fonts In Xml". -->

  <font
    app:font="@font/montserrat_regular_font"
    app:fontStyle="normal"
    app:fontWeight="400" />

  <font
    app:font="@font/montserrat_semibold_font"
    app:fontStyle="normal"
    app:fontWeight="600" />

  <font
    app:font="@font/montserrat_bold_font"
    app:fontStyle="normal"
    app:fontWeight="700" />

</font-family>
